: zIndex + 1,\n\n        // div center 정렬\n        top: '50%',\n        left: '50%',\n        transform: 'translate(-50%, -50%)',\n        msTransform: 'translate(-50%, -50%)',\n        webkitTransform: 'translate(-50%, -50%)'\n    });\n}\n\n// Element 에 style 한번에 오브젝트로 설정하는 함수 추가\nElement.prototype.setStyle = function(styles) {\n    for (var k in styles) this.style[k] = styles[k];\n    return this;\n};\n\ndocument.getElementById('popup_open_btn').addEventListener('click', function() {\n    // 모달창 띄우기\n    modal('my_modal');\n});\n\n  </script>\n  \n  \n  <style scoped>\n.ex1 {\n        position: relative;\n        background-color: #181B21;\n        border-radius: 20px;\n        height: 520px;\n        margin-bottom: 10px;\n}\n.ex1_center_text {\n    position: relative;\n    text-align: center;\n    color: white;\n    font-size: 40px;\n    padding-top: 50px;\n    font-weight: bold;\n}\n/*-------------------------------- 위에 박스 세개---------------------------- */\n.box1 {\n    position: relative;\n    background-color: #6790CE;\n    border-radius: 20px;\n    width: 443px;\n    height: 290px;\n    margin-top: 110px;\n    margin-left: 10px;\n    float: left;\n}\n.box1_text {\n    position: relative;\n    text-align: center;\n    color: white;\n    font-size: 40px;\n    padding-top: 40px;\n}\n\n.box2 {\n    position: relative;\n    background-color: #C75B6F;\n    border-radius: 20px;\n    width: 443px;\n    height: 290px;\n    margin-top: 110px;\n    margin-left: 10px;\n    float: left;\n}\n.box2_text {\n    position: relative;\n    text-align: center;\n    color: white;\n    font-size: 40px;\n    padding-top: 40px;\n}\n\n.box3 {\n    position: relative;\n    background-color: #8EC693;\n    border-radius: 20px;\n    width: 443px;\n    height: 290px;\n    margin-top: 110px;\n    margin-left: 10px;\n    float: left;\n}\n.box3_text {\n    position: relative;\n    text-align: center;\n    color: white;\n    font-size: 40px;\n    padding-top: 40px;\n}\n/* ---------------------